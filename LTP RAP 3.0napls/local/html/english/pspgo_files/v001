$(document).ready(MJ_init);function MJ_init(){common();if(VersionTool>0){if(VersionTool>0x060001){$('#DS3USBList').bind('readAddr',function(){$('#DS3MAC').html(GetControllerinfo($("#DS3USBList :selected").val(),0,2));$('#ds3addr').html(GetLocalBTinfo($("#DS3USBList :selected").val(),1))});$('#DS3USBList').change(function(){$('#DS3USBList').trigger('readAddr')});$('#PSPGoList').bind('readAddr',function(){$('#PSPGoMAC').html(GetControllerinfo($("#PSPGoList :selected").val(),0,2))});$('#PSPGoList').change(function(){$('#PSPGoList').trigger('readAddr')});$('#BTPSPGOPair').click(function(){BTPair($("#DS3USBList :selected").val(),$('#PSPGoMAC').html());BTPair($("#PSPGoList :selected").val(),$('#DS3MAC').html());$('#DS3USBList').trigger('readAddr');mmm()});GetInfo()}}}function GetInfo(){var PSPGoList=0;var DS3USBList=0;$('#PSPGoList').html('<option value="0">'+$('#NoPSPGo').html()+'</option>');$('#DS3USBList').html('<option value="0">'+$('#NoDS3USB').html()+'</option>');for(var i=0;i<GetDeviceCount();i++){switch(GetDeviceType(i)){case"0":var DeviceName=$('#DS3USBName').html();if(DS3USBList==0){$('#DS3USBList').html('')}DS3USBList++;$('<option value="'+i+'">'+DS3USBList+'.'+DeviceName+'</option>').appendTo('#DS3USBList');$('#DS3USBList').trigger('readAddr');break;case"7":var DeviceName=$('#PSPGoName').html();if(PSPGoList==0){$('#PSPGoList').html('')}PSPGoList++;$('<option value="'+i+'">'+PSPGoList+'.'+DeviceName+'</option>').appendTo('#PSPGoList');$('#PSPGoList').trigger('readAddr');break;default:break}}}function EventHander(){switch(arguments[0]){case 1:GetInfo();break;default:break}}
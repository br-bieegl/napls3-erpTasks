$(document).ready(MJ_init);function MJ_init(){common();if(VersionTool>0){if(VersionTool>0x060000){$('#InstllDriver').click(function(){$('#installlog').html('<h1 style="width:100%; color: #9F6000;background-color: #FEEFB3;font-size:20px;">This may take a few seconds.Please wait a moment!</h1>');DriverInstall()});$('#RevmoveDriver').click(function(){mmm();$('#installlog').html('<p><h1 style="width:100%;color: #9F6000;background-color: #FEEFB3;font-size:20px;">This may take a few seconds,Please wait a moment!</h1></p>');DriverRemove()})}else if(VersionTool>=0x050000){$('#InstllDriver').click(function(){$('#installlog').html('<h1 style="width:100%; color: #9F6000;background-color: #FEEFB3;font-size:20px;">This may take a few seconds.Please wait a moment!</h1>');window.external.configDevice(6,0,1,0,0,0)});$('#RevmoveDriver').click(function(){Mconfig();$('#installlog').html('<p><h1 style="width:100%;color: #9F6000;background-color: #FEEFB3;font-size:20px;">This may take a few seconds,Please wait a moment!</h1></p>');window.external.configDevice(6,0,0,0,0,0)})}else{$('#InstllDriver').click(function(){alert('Need DS3 tool ver 0.5+')});$('#RevmoveDriver').click(function(){alert('Need DS3 tool ver 0.5+')})}}}function Drvierlog(EventDescription,EventType,ErrorCode){var message='';switch(EventType){case 0:if(ErrorCode==0){message='<p style="color: #4F8A10;background-color: #DFF2BF;">'+'SUCCESS:'+EventDescription+'</p>'}else{message='<p style="color: #4F8A10;background-color: #DFF2BF;">'+'SUCCESS:'+EventDescription+'. Error code:'+ErrorCode+'</p>'}break;case 1:if(ErrorCode==0){message='<p style="color: #00529B;background-color: #BDE5F8">'+'INFO:'+EventDescription+'</p>'}else{message='<p style="color: #00529B;background-color: #BDE5F8">'+'INFO:'+EventDescription+'. Error code:'+ErrorCode+'</p>'}break;case 2:if(ErrorCode==0){message='<p style="color: #9F6000;background-color: #FEEFB3;">'+'WARNING:'+EventDescription+'</p>'}else{message='<p style="color: #9F6000;background-color: #FEEFB3;">'+'WARNING:'+EventDescription+'. Error code:'+ErrorCode+'</p>'}break;case 3:if(ErrorCode==0){message='<p style="color: #D8000C;background-color: #FFBABA;">'+'ERROR:'+EventDescription+'</p>'}else{message='<p style="color: #D8000C;background-color: #FFBABA;">'+'ERROR:'+EventDescription+'. Error code:'+ErrorCode+'</p>'}break;default:break}$(message).appendTo('#installlog')}
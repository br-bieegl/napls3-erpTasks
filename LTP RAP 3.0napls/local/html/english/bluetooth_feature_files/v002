$(document).ready(MJ_init);function MJ_init(){common();if(VersionTool>0x060000){var flag=0x00;for(var i=0;i<GetDeviceCount();i++){if(GetDeviceType(i)==1){flag=parseInt(GetLocalBTinfo(i,5));break}}for(var i=0;i<32;i++){if((flag&(1<<i))!=0){$('#Featureid_'+i).html('<img src="./bluetooth_feature_files/q_correct.png" width="12" height="12">')}else{$('#Featureid_'+i).html('<img src="./bluetooth_feature_files/q_incorrect.png" width="12" height="12">')}}flag=parseInt(GetLocalBTinfo(0,6));for(var i=32;i<48;i++){if((flag&(1<<(i-32)))!=0){$('#Featureid_'+i).html('<img src="./bluetooth_feature_files/q_correct.png" width="12" height="12">')}else{$('#Featureid_'+i).html('<img src="./bluetooth_feature_files/q_incorrect.png" width="12" height="12">')}}}}
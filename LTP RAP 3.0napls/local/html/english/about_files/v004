$(document).ready(MJ_init);function MJ_init(){common();if(VersionTool>0){$("#ds3toolver").html((VersionTool>>20).toString(16)+'.'+((VersionTool>>16)&0x0f).toString(16)+'.'+(Array(4).join(0)+(VersionTool&0xffff).toString(16)).slice(-4));$("#driverver").html((VersionDriver>>20).toString(16)+'.'+((VersionDriver>>16)&0x0f).toString(16)+'.'+(Array(4).join(0)+(VersionDriver&0xffff).toString(16)).slice(-4));if(VersionTool>0x060000){$('#CheckUpdate').click(function(){if(isNeedUpdata()=="True"){NeedUpdate()}else{$('#info').load('/quick_start/noupdate').dialog({show:'blind',hide:'blind',title:"No file need update",modal:false,autoOpen:true,draggable:true,height:150,width:340,buttons:{Ok:function(){$(this).dialog("close")}}})}})}else if(VersionTool>=0x050000){$('#CheckUpdate').click(function(){Mconfig();if(window.external.configDevice(8,0,1,0,0,0)!=0){if(VersionTool<0x060000){NeedUpdate()}}else{$('#info').load('/quick_start/noupdate').dialog({show:'blind',hide:'blind',title:"No file need update",modal:false,autoOpen:true,draggable:true,height:100,width:340})}})}else{$('#CheckUpdate').click(function(){window.external.link("http://www.motioninjoy.com/update/log")})}}}